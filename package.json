  "scripts": {
    "build": "npm run clean:dist && npm run sass",
    "build:prod": "npm run clean:dist && npm run sass && webpack --prod --config config/webpack.config.ts",
    "start": "npm run build && npm run server",
    "start:prod": "npm run build:prod && node server/index.ts",
    "webdev": "webpack-dev-server --config config/webpack.config.ts",
    "webdev:hmr": "npm run webdev --hotOnly --hot --inline",
    "server": "npm-run-all -p -r webdev:hmr sass:watch",
    "sass": "node-sass \"src/styles.scss\" \"dist_root/styles.css\" --include-path node_modules --output-style compressed -q",
    "sass:watch": "node-sass -w \"src/styles.scss\" \"dist_root/styles.css\" --include-path node_modules --output-style compressed -q",
    "clean:dist": "rimraf -- dist",
    "test": "karma start config/karma.conf.js",
    "lint": "tslint -p tslint.json src/**/*.ts test/**/*.ts",
    "dev:server": "tsc --project tsconfig.server.json && node ./server_compiled/index.js"
  },
